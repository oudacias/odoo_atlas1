      <field name="model">petrol.station</field>
      <field name="inherit_id" ref="petrol_station_doosys.list_petrol_station" />
      <field name="arch" type="xml">
          <field name="pricelist_id" position="after">
              <field name="magasin_code" />
              <field name="warehouse_sap" />
              <field name="compte_commission" />
              <field name="compte_caisse" />
              <field name="compte_cheque" />
              <field name="compte_carte_credite" />
              <field name="compte_client_comptant" />
